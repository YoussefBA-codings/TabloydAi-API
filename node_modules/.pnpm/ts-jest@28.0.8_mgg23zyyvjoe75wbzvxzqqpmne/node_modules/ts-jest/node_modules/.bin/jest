#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/av/TabloydAi/api/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/av/TabloydAi/api/node_modules/.pnpm/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../jest@28.1.3_ghv2zugsw3zjg5rog5rhyka5ja/node_modules/jest/bin/jest.js" "$@"
else
  exec node  "$basedir/../../../../../jest@28.1.3_ghv2zugsw3zjg5rog5rhyka5ja/node_modules/jest/bin/jest.js" "$@"
fi
