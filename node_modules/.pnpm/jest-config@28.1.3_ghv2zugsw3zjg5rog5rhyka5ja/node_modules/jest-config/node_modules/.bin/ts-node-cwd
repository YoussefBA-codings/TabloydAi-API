#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/av/TabloydAi/api/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/av/TabloydAi/api/node_modules/.pnpm/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../ts-node@10.9.1_rcvzjt2q3f6gordb34f5djrkie/node_modules/ts-node/dist/bin-cwd.js" "$@"
else
  exec node  "$basedir/../../../../../ts-node@10.9.1_rcvzjt2q3f6gordb34f5djrkie/node_modules/ts-node/dist/bin-cwd.js" "$@"
fi
